@prefix : <http://modelmeth.nist.gov/manufacturing#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/1.1/schema/qudt#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://modelmeth.nist.gov/manufacturing> .

<http://modelmeth.nist.gov/manufacturing> rdf:type owl:Ontology ;
                                          
                                          owl:imports <http://model-meth.nist.gov/1.0/schema/dtype> ,
                                                      <http://model-meth.nist.gov/1.1/schema/qudt> ,
                                                      <http://model-meth.nist.gov/1.2/schema/vaem> .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://modelmeth.nist.gov/manufacturing#hasUnits

:hasUnits rdf:type owl:AnnotationProperty .



###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled

<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .





#################################################################
#
#    Datatypes
#
#################################################################


###  http://modelmeth.nist.gov/manufacturing#characteristicObjectiveType

:characteristicObjectiveType rdf:type rdfs:Datatype ;
                             
                             owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                                   owl:oneOf [ rdf:type rdf:List ;
                                                               rdf:first "avoided_characteristic"^^xsd:NMTOKEN ;
                                                               rdf:rest [ rdf:type rdf:List ;
                                                                          rdf:first "sought_characteristic"^^xsd:NMTOKEN ;
                                                                          rdf:rest rdf:nil
                                                                        ]
                                                             ]
                                                 ] .



###  http://modelmeth.nist.gov/manufacturing#processCharacteristicObjectiveType

:processCharacteristicObjectiveType rdf:type rdfs:Datatype ;
                                    
                                    owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                                          owl:oneOf [ rdf:type rdf:List ;
                                                                      rdf:first "avoided_processCharacteristic"^^xsd:NMTOKEN ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first "sought_processCharacteristic"^^xsd:NMTOKEN ;
                                                                                 rdf:rest rdf:nil
                                                                               ]
                                                                    ]
                                                        ] .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://modelmeth.nist.gov/manufacturing#appliesTo

:appliesTo rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#definedBy

:definedBy rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#defines

:defines rdf:type owl:ObjectProperty ;
         
         owl:inverseOf :definedBy .



###  http://modelmeth.nist.gov/manufacturing#determinedBy

:determinedBy rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#hasCharacteristic

:hasCharacteristic rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#hasCost

:hasCost rdf:type owl:ObjectProperty ;
         
         rdfs:domain :Resource .



###  http://modelmeth.nist.gov/manufacturing#hasDuration

:hasDuration rdf:type owl:ObjectProperty ;
             
             rdfs:domain :Process .



###  http://modelmeth.nist.gov/manufacturing#hasFeedRate

:hasFeedRate rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:range :CuttingFeedRate ;
             
             rdfs:domain :MachiningProcess .



###  http://modelmeth.nist.gov/manufacturing#hasFinishedPart

:hasFinishedPart rdf:type owl:AsymmetricProperty ,
                          owl:FunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:range :FinishedPart ;
                 
                 rdfs:domain :Workpiece ;
                 
                 owl:inverseOf :hasWorkpiece .



###  http://modelmeth.nist.gov/manufacturing#hasMaterial

:hasMaterial rdf:type owl:ObjectProperty ;
             
             rdfs:range :CuttingToolMaterial ,
                        :Material .



###  http://modelmeth.nist.gov/manufacturing#hasProcessCharacteristic

:hasProcessCharacteristic rdf:type owl:ObjectProperty ;
                          
                          owl:inverseOf :hasProcessContext .



###  http://modelmeth.nist.gov/manufacturing#hasProcessContext

:hasProcessContext rdf:type owl:ObjectProperty ;
                   
                   rdfs:range :Process .



###  http://modelmeth.nist.gov/manufacturing#hasQuantity

:hasQuantity rdf:type owl:ObjectProperty ;
             
             rdfs:subPropertyOf owl:topObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#hasSymbol

:hasSymbol rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#hasValue

:hasValue rdf:type owl:AsymmetricProperty ,
                   owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#hasWorkpiece

:hasWorkpiece rdf:type owl:InverseFunctionalProperty ,
                       owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#isDirectlyProportionalTo

:isDirectlyProportionalTo rdf:type owl:ObjectProperty ,
                                   owl:SymmetricProperty .



###  http://modelmeth.nist.gov/manufacturing#isInverselyProportionalTo

:isInverselyProportionalTo rdf:type owl:ObjectProperty ,
                                    owl:SymmetricProperty .



###  http://modelmeth.nist.gov/manufacturing#isSpecifiedBy

:isSpecifiedBy rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#provides

:provides rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#quantifiedBy

:quantifiedBy rdf:type owl:ObjectProperty .



###  http://modelmeth.nist.gov/manufacturing#quantifies

:quantifies rdf:type owl:ObjectProperty ;
            
            owl:inverseOf :quantifiedBy .



###  http://modelmeth.nist.gov/manufacturing#referencesEquation

:referencesEquation rdf:type owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ,
                             owl:ObjectProperty ;
                    
                    rdfs:range :Equation ;
                    
                    rdfs:domain :Model .



###  http://modelmeth.nist.gov/manufacturing#referencesProperty

:referencesProperty rdf:type owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ,
                             owl:ObjectProperty ;
                    
                    rdfs:domain :Model ;
                    
                    rdfs:range :ModelProperty .



###  http://modelmeth.nist.gov/manufacturing#representsProperty

:representsProperty rdf:type owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ,
                             owl:ObjectProperty ;
                    
                    rdfs:range :ModelProperty ;
                    
                    rdfs:domain :Symbol ;
                    
                    rdfs:subPropertyOf owl:topObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://modelmeth.nist.gov/manufacturing#hasDimensionalConsistency

:hasDimensionalConsistency rdf:type owl:DatatypeProperty ;
                           
                           rdfs:range xsd:boolean .



###  http://modelmeth.nist.gov/manufacturing#hasObjectiveType

:hasObjectiveType rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://modelmeth.nist.gov/manufacturing#Abstract

:Abstract rdf:type owl:Class ;
          
          rdfs:comment "Aspect with neither a spatial nor a temporal location" .



###  http://modelmeth.nist.gov/manufacturing#Alloy

:Alloy rdf:type owl:Class ;
       
       rdfs:subClassOf :Material ;
       
       rdfs:comment "Material composed of multiple metals or metals and other elements" .



###  http://modelmeth.nist.gov/manufacturing#AlternativeHypothesis

:AlternativeHypothesis rdf:type owl:Class ;
                       
                       rdfs:subClassOf :Condition ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasValue ;
                                         owl:allValuesFrom :True
                                       ] .



###  http://modelmeth.nist.gov/manufacturing#AlternativeTypicalValue

:AlternativeTypicalValue rdf:type owl:Class ;
                         
                         rdfs:subClassOf :TypicalValue .



###  http://modelmeth.nist.gov/manufacturing#AnalysisTechnique

:AnalysisTechnique rdf:type owl:Class ;
                   
                   rdfs:subClassOf :StatisticalAnalysis .



###  http://modelmeth.nist.gov/manufacturing#Average

:Average rdf:type owl:Class ;
         
         owl:equivalentClass :Mean ;
         
         rdfs:subClassOf :CommonTypicalValue .



###  http://modelmeth.nist.gov/manufacturing#Boring

:Boring rdf:type owl:Class ;
        
        rdfs:subClassOf :SinglePointCutting .



###  http://modelmeth.nist.gov/manufacturing#CauchyDistribution

:CauchyDistribution rdf:type owl:Class ;
                    
                    rdfs:subClassOf :Distribution .



###  http://modelmeth.nist.gov/manufacturing#Characteristic

:Characteristic rdf:type owl:Class ;
                
                rdfs:subClassOf :StatisticalAnalysis .



###  http://modelmeth.nist.gov/manufacturing#CommonTypicalValue

:CommonTypicalValue rdf:type owl:Class ;
                    
                    rdfs:subClassOf :TypicalValue .



###  http://modelmeth.nist.gov/manufacturing#Condition

:Condition rdf:type owl:Class ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( :AlternativeHypothesis
                                               :NullHypothesis
                                             )
                               ] ;
           
           rdfs:subClassOf :Characteristic .



###  http://modelmeth.nist.gov/manufacturing#ConfidenceLevel

:ConfidenceLevel rdf:type owl:Class ;
                 
                 rdfs:subClassOf :Characteristic .



###  http://modelmeth.nist.gov/manufacturing#Cost

:Cost rdf:type owl:Class ;
      
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :isDirectlyProportionalTo ;
                            owl:someValuesFrom :CumulativeUse
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isDirectlyProportionalTo ;
                            owl:someValuesFrom :SurfaceRoughness
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isDirectlyProportionalTo ;
                            owl:someValuesFrom :ToolWear
                          ] ;
      
      rdfs:subClassOf :Abstract ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasObjectiveType ;
                        owl:hasValue "avoided_characteristic"
                      ] ;
      
      rdfs:comment "Monetary value of an aspect of the smart manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#CriticalRegion

:CriticalRegion rdf:type owl:Class ;
                
                rdfs:subClassOf :Characteristic .



###  http://modelmeth.nist.gov/manufacturing#CumulativeUse

:CumulativeUse rdf:type owl:Class ;
               
               rdfs:subClassOf :ProcessCharacteristic .



###  http://modelmeth.nist.gov/manufacturing#CuttingFeedRate

:CuttingFeedRate rdf:type owl:Class ;
                 
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :isDirectlyProportionalTo ;
                                       owl:someValuesFrom :SurfaceRoughness
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :isDirectlyProportionalTo ;
                                       owl:someValuesFrom :CuttingForce
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :isDirectlyProportionalTo ;
                                       owl:someValuesFrom :ToolWear
                                     ] ;
                 
                 rdfs:subClassOf :MachiningProcessParameter ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasQuantity ;
                                   owl:hasValue :LinearVelocity
                                 ] ;
                 
                 rdfs:comment "Speed with which a part moves through a machine" .



###  http://modelmeth.nist.gov/manufacturing#CuttingFluidType

:CuttingFluidType rdf:type owl:Class ;
                  
                  rdfs:subClassOf :MachiningProcessParameter ;
                  
                  rdfs:comment "Coolant and lubricant designed for manufacturing processes" .



###  http://modelmeth.nist.gov/manufacturing#CuttingForce

:CuttingForce rdf:type owl:Class ;
              
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :isDirectlyProportionalTo ;
                                    owl:someValuesFrom :CuttingFeedRate
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isDirectlyProportionalTo ;
                                    owl:someValuesFrom :CuttingSpeed
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isDirectlyProportionalTo ;
                                    owl:someValuesFrom :DepthOfCut
                                  ] ;
              
              rdfs:subClassOf :ProcessCharacteristic ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasQuantity ;
                                owl:hasValue qudt:Force
                              ] ;
              
              rdfs:comment "Force needed to complete manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#CuttingInsert

:CuttingInsert rdf:type owl:Class ;
               
               rdfs:subClassOf :Equipment ;
               
               rdfs:comment "Removable, replaceable equipment piece that performs cutting operation" .



###  http://modelmeth.nist.gov/manufacturing#CuttingSpeed

:CuttingSpeed rdf:type owl:Class ;
              
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :isDirectlyProportionalTo ;
                                    owl:someValuesFrom :CuttingForce
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isDirectlyProportionalTo ;
                                    owl:someValuesFrom :ToolWear
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isDirectlyProportionalTo ;
                                    owl:someValuesFrom :SurfaceRoughness
                                  ] ;
              
              rdfs:subClassOf :MachiningProcessParameter ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasQuantity ;
                                owl:hasValue :LinearVelocity
                              ] ;
              
              rdfs:comment "Speed with which a workpiece is cut by the machine" .



###  http://modelmeth.nist.gov/manufacturing#CuttingTool

:CuttingTool rdf:type owl:Class ;
             
             rdfs:subClassOf :Equipment .



###  http://modelmeth.nist.gov/manufacturing#CuttingToolMaterial

:CuttingToolMaterial rdf:type owl:Class ;
                     
                     rdfs:subClassOf :CuttingTool .



###  http://modelmeth.nist.gov/manufacturing#DepthOfCut

:DepthOfCut rdf:type owl:Class ;
            
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :isDirectlyProportionalTo ;
                                  owl:someValuesFrom :SurfaceRoughness
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :isDirectlyProportionalTo ;
                                  owl:someValuesFrom :CuttingForce
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :isDirectlyProportionalTo ;
                                  owl:someValuesFrom :ToolWear
                                ] ;
            
            rdfs:subClassOf :ProcessCharacteristic ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasQuantity ;
                              owl:hasValue qudt:Length
                            ] ;
            
            rdfs:comment "Measure of the magnitude of the subtractive process" .



###  http://modelmeth.nist.gov/manufacturing#Distribution

:Distribution rdf:type owl:Class ;
              
              rdfs:subClassOf :Characteristic .



###  http://modelmeth.nist.gov/manufacturing#Drift

:Drift rdf:type owl:Class ;
       
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :isDirectlyProportionalTo ;
                             owl:someValuesFrom :CumulativeUse
                           ] ;
       
       rdfs:subClassOf :ProcessCharacteristic ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasQuantity ;
                         owl:hasValue qudt:Length
                       ] ;
       
       rdfs:comment "Machine inaccuracy acquired over time and multiple repetitions of the manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#Drilling

:Drilling rdf:type owl:Class ;
          
          rdfs:subClassOf :MultiPointCutting .



###  http://modelmeth.nist.gov/manufacturing#Equation

:Equation rdf:type owl:Class ;
          
          rdfs:subClassOf :ModelAbstraction ;
          
          rdfs:comment "Function describing the relation between ModelProperties" .



###  http://modelmeth.nist.gov/manufacturing#Equipment

:Equipment rdf:type owl:Class ;
           
           rdfs:subClassOf :Resource ;
           
           rdfs:comment "Machine(s) used to turn workpiece into finished part" .



###  http://modelmeth.nist.gov/manufacturing#EquipmentCharacteristic

:EquipmentCharacteristic rdf:type owl:Class ;
                         
                         rdfs:subClassOf :Physical ;
                         
                         rdfs:comment "Equipment aspect that contributes to the manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#ExponentialDistribution

:ExponentialDistribution rdf:type owl:Class ;
                         
                         rdfs:subClassOf :Distribution .



###  http://modelmeth.nist.gov/manufacturing#Facing

:Facing rdf:type owl:Class ;
        
        rdfs:subClassOf :Turning ;
        
        rdfs:comment "A subtractive manufacturing process in which the part is rotated and a single point cutting tool is moved perpendicular to the axis of rotation." .



###  http://modelmeth.nist.gov/manufacturing#False

:False rdf:type owl:Class ;
       
       rdfs:subClassOf :Characteristic .



###  http://modelmeth.nist.gov/manufacturing#FinalDimension

:FinalDimension rdf:type owl:Class ;
                
                rdfs:subClassOf :WorkpieceCharacteristic ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasQuantity ;
                                  owl:hasValue qudt:Length
                                ] .



###  http://modelmeth.nist.gov/manufacturing#FinishedPart

:FinishedPart rdf:type owl:Class ;
              
              rdfs:subClassOf :Physical ;
              
              rdfs:comment "A manufactured item ready for use by the customer, or to be included in an assembly." .



###  http://modelmeth.nist.gov/manufacturing#Fixture

:Fixture rdf:type owl:Class ;
         
         rdfs:subClassOf :Equipment .



###  http://modelmeth.nist.gov/manufacturing#GraphicalAnalysis

:GraphicalAnalysis rdf:type owl:Class ;
                   
                   rdfs:subClassOf :AnalysisTechnique .



###  http://modelmeth.nist.gov/manufacturing#Grinding

:Grinding rdf:type owl:Class ;
          
          rdfs:subClassOf :MultiPointCutting .



###  http://modelmeth.nist.gov/manufacturing#HypothesisTest

:HypothesisTest rdf:type owl:Class ;
                
                rdfs:subClassOf :QuantitativeAnalysis ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :isSpecifiedBy ;
                                  owl:someValuesFrom :TestStatistic
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :quantifies ;
                                  owl:someValuesFrom :Uncertainty
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :definedBy ;
                                  owl:someValuesFrom :Condition
                                ] .



###  http://modelmeth.nist.gov/manufacturing#InitialDimension

:InitialDimension rdf:type owl:Class ;
                  
                  rdfs:subClassOf :WorkpieceCharacteristic ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :hasQuantity ;
                                    owl:hasValue qudt:Length
                                  ] .



###  http://modelmeth.nist.gov/manufacturing#Interval

:Interval rdf:type owl:Class ;
          
          rdfs:subClassOf :Characteristic ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasValue ;
                            owl:someValuesFrom [ rdf:type owl:Class ;
                                                 owl:intersectionOf ( :LowerValue
                                                                      :UpperValue
                                                                    )
                                               ]
                          ] .



###  http://modelmeth.nist.gov/manufacturing#IntervalEstimate

:IntervalEstimate rdf:type owl:Class ;
                  
                  rdfs:subClassOf :QuantitativeAnalysis ,
                                  [ rdf:type owl:Class ;
                                    owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                           owl:onProperty :provides ;
                                                           owl:someValuesFrom :Interval
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :quantifies ;
                                                           owl:someValuesFrom :Uncertainty
                                                         ]
                                                       )
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :definedBy ;
                                    owl:someValuesFrom [ rdf:type owl:Class ;
                                                         owl:intersectionOf ( :PointEstimate
                                                                              :Uncertainty
                                                                            )
                                                       ]
                                  ] .



###  http://modelmeth.nist.gov/manufacturing#Labor

:Labor rdf:type owl:Class ;
       
       rdfs:subClassOf :Resource ;
       
       rdfs:comment "Personnel responsible for setup and overview of smart manufaturing process" .



###  http://modelmeth.nist.gov/manufacturing#LevelOfConfidence

:LevelOfConfidence rdf:type owl:Class ;
                   
                   rdfs:subClassOf :Characteristic .



###  http://modelmeth.nist.gov/manufacturing#LocationParameterEstimate

:LocationParameterEstimate rdf:type owl:Class ;
                           
                           rdfs:subClassOf :QuantitativeAnalysis ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :hasValue ;
                                             owl:someValuesFrom :TypicalValue
                                           ] .



###  http://modelmeth.nist.gov/manufacturing#LognormalDistribution

:LognormalDistribution rdf:type owl:Class ;
                       
                       rdfs:subClassOf :Distribution .



###  http://modelmeth.nist.gov/manufacturing#LowerValue

:LowerValue rdf:type owl:Class ;
            
            rdfs:subClassOf :Value .



###  http://modelmeth.nist.gov/manufacturing#MachineBed

:MachineBed rdf:type owl:Class ;
            
            rdfs:subClassOf :MachineCharacteristic .



###  http://modelmeth.nist.gov/manufacturing#MachineCharacteristic

:MachineCharacteristic rdf:type owl:Class ;
                       
                       rdfs:subClassOf :EquipmentCharacteristic ;
                       
                       rdfs:comment "Machine aspect that contributes to the manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#MachinePower

:MachinePower rdf:type owl:Class ;
              
              rdfs:subClassOf :MachineCharacteristic .



###  http://modelmeth.nist.gov/manufacturing#MachineSpindle

:MachineSpindle rdf:type owl:Class ;
                
                rdfs:subClassOf :MachineCharacteristic .



###  http://modelmeth.nist.gov/manufacturing#MachineTool

:MachineTool rdf:type owl:Class ;
             
             rdfs:subClassOf :Equipment .



###  http://modelmeth.nist.gov/manufacturing#MachineToolChanger

:MachineToolChanger rdf:type owl:Class ;
                    
                    rdfs:subClassOf :MachineCharacteristic .



###  http://modelmeth.nist.gov/manufacturing#Machinery

:Machinery rdf:type owl:Class ;
           
           rdfs:subClassOf :Resource ;
           
           rdfs:comment "Machines needed to produce part" .



###  http://modelmeth.nist.gov/manufacturing#MachiningProcess

:MachiningProcess rdf:type owl:Class ;
                  
                  rdfs:subClassOf :ManufacturingProcess ;
                  
                  rdfs:comment "A subtractive manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#MachiningProcessParameter

:MachiningProcessParameter rdf:type owl:Class ;
                           
                           rdfs:subClassOf :ProcessParameter ;
                           
                           rdfs:comment "A controllable characteristic that can be adjusted for different final results" .



###  http://modelmeth.nist.gov/manufacturing#ManufacturingProcess

:ManufacturingProcess rdf:type owl:Class ;
                      
                      rdfs:subClassOf :Process ;
                      
                      rdfs:comment "A process that is part of creating a manufactured artifact" .



###  http://modelmeth.nist.gov/manufacturing#Material

:Material rdf:type owl:Class ;
          
          rdfs:subClassOf :WorkpieceCharacteristic ;
          
          rdfs:comment "Substance that the Workpiece/FinishedPart is made up of" .



###  http://modelmeth.nist.gov/manufacturing#Mean

:Mean rdf:type owl:Class ;
      
      rdfs:subClassOf :CommonTypicalValue ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :appliesTo ;
                        owl:allValuesFrom :UnivariateData
                      ] .



###  http://modelmeth.nist.gov/manufacturing#Median

:Median rdf:type owl:Class ;
        
        rdfs:subClassOf :CommonTypicalValue ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :appliesTo ;
                          owl:allValuesFrom :UnivariateData
                        ] .



###  http://modelmeth.nist.gov/manufacturing#MidMean

:MidMean rdf:type owl:Class ;
         
         rdfs:subClassOf :AlternativeTypicalValue .



###  http://modelmeth.nist.gov/manufacturing#Milling

:Milling rdf:type owl:Class ;
         
         rdfs:subClassOf :MultiPointCutting .



###  http://modelmeth.nist.gov/manufacturing#Mode

:Mode rdf:type owl:Class ;
      
      rdfs:subClassOf :CommonTypicalValue ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :appliesTo ;
                        owl:allValuesFrom :UnivariateData
                      ] .



###  http://modelmeth.nist.gov/manufacturing#Model

:Model rdf:type owl:Class ;
       
       rdfs:subClassOf :ModelAbstraction ;
       
       rdfs:comment "A simulation of a manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#ModelAbstraction

:ModelAbstraction rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Abstract ;
                  
                  rdfs:comment "A conceptualization of a manufacturing process" .



###  http://modelmeth.nist.gov/manufacturing#ModelParameter

:ModelParameter rdf:type owl:Class ;
                
                rdfs:subClassOf :ModelProperty ;
                
                rdfs:comment "a ModelProperty whose value can be specified so as to specify a context of the analysis." .



###  http://modelmeth.nist.gov/manufacturing#ModelProperty

:ModelProperty rdf:type owl:Class ;
               
               rdfs:subClassOf :ModelAbstraction ;
               
               owl:disjointWith :Symbol ;
               
